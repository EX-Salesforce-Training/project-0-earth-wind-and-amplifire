 * Description: Trigger for the Job__c object
 */

trigger JobTrigger on Job__c (before update) {
    
    switch on trigger.operationType {
        when BEFORE_UPDATE {
            JobTriggerHandler.completed(trigger.newMap);
        }
    }

}